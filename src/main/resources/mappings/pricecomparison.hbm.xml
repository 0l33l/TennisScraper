<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="com.springhibernate.integration.model.PriceComparison" table="PRICECOMPARISON">
    <id name="id" column="idpricecomparison">
      <generator class="increment"/>
    </id>

    <many-to-one name="product" class="com.springhibernate.integration.model.Product" lazy="false" column="idproduct"/>

    <property name="url" column="URL"/>

    <property name="price" column="PRICE"/>

    <property name="reseller" column="RESELLER"/>

  </class>
</hibernate-mapping>